<Window x:Class="ORO_Lb5.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ORO_Lb5"
        mc:Ignorable="d"
        Title="MainWindow" Height="818" Width="1100" Background="#FF6A6A6A">
    <Grid>
        <Image x:Name="Video" HorizontalAlignment="Left" Height="300" Margin="50,50,0,0" VerticalAlignment="Top" Width="300" Stretch="Fill"/>
        <Image x:Name="CurrentFrame" Height="300" Width="300" Margin="400,50,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Stretch="Fill"/>
        <Image x:Name="ContourG" Height="300" Width="300" Margin="50,400,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Stretch="Fill"/>
        <Image x:Name="ContourM" Height="300" Width="300" Margin="400,400,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Stretch="Fill"/>
        <Image x:Name="ContourS" Height="300" Width="300" Margin="750,400,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Stretch="Fill"/>
        <Button x:Name="StartButton" Content="Запуск" HorizontalAlignment="Left" Margin="800,150,0,0" VerticalAlignment="Top" Width="200" Height="100" FontSize="48" Background="#FF59D1C8" Click="Button_Click"/>

    </Grid>
</Window>
